//ipfetch rolling
//Information for esp package manager
ESP_PACKAGE
{
    name = ipfetch
    description = Neofetch like tool that can look up IPs.
    type = Git
    version = rolling
    cloneURL = https://github.com/trakBan/ipfetch
    updateURL = https://raw.githubusercontent.com/Mrcarrot1/esp/main/ExamplePackages/ipfetch.esp
    INSTALL_COMMANDS
    [
        sudo sh setup.sh
    ]
    UNINSTALL_COMMANDS
    [
        sudo rm /usr/bin/ipfetch
        sudo rm -r /usr/share/ipfetch
    ]
}